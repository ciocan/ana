{
  "presets": ["next/babel"],
  "plugins": [
    ["styled-components", { "ssr": true }],
    "react-require",
    "inline-react-svg",
    [
      "module-resolver",
      {
        "root": ["."],
        "alias": {
          "@components": "./src/components",
          "@hooks": "./src/hooks",
          "@lib": "./src/lib",
          "@svg": "./src/svg"
        }
      },
      "@babel/plugin-proposal-optional-chaining"
    ]
  ]
}
